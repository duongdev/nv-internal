{
  "name": "nv-internal",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "pnpm -w -r --workspace-concurrency=1 test",
    "test:api": "pnpm -w --filter @nv-internal/api test",
    "test:api:verbose": "pnpm -w --filter @nv-internal/api run test:verbose",
    "test:coverage": "pnpm -w -r test -- --coverage",
    
    "biome:lint": "biome lint",
    "biome:check": "biome check --write",
    "biome:format": "biome format --write"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [],
  "author": "Dustin Do <dustin.do95@gmail.com>",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@vercel/node": "^5.3.17",
    "typescript": "^5.9.3",
    "vercel": "^46.1.1"
  },
  "resolutions": {
    "lightningcss": "1.27.0",
    "typescript": "5.9.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "expo-modules-*",
        "typescript"
      ]
    },
    "overrides": {
      "react": "19.1.0",
      "typescript": "5.9.3"
    }
  }
}
