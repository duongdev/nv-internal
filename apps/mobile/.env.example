# Clerk Authentication
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_key_here

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000

# Google Maps API Keys (Platform-specific)
# Get separate keys for iOS and Android from Google Cloud Console
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY_IOS=your_ios_key_here
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY_ANDROID=your_android_key_here

# PostHog Analytics (Optional)
EXPO_PUBLIC_POSTHOG_API_KEY=your_posthog_key_here
EXPO_PUBLIC_POSTHOG_HOST=https://app.posthog.com
EXPO_PUBLIC_POSTHOG_ENABLED=true

# Environment Indicator (managed by EAS build profiles)
# Don't set this manually - it's set by eas.json
# EXPO_PUBLIC_ENV=development